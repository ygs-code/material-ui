    "git": "node  -r @babel/register  ./bin/git.js",
    "commit": "git-cz",
    "commitlint": "npx --no-install commitlint --edit $1",
    "lint-staged": "lint-staged",
    "husky-install": "husky install",
    "lint-style": "stylelint \"client/**/*.(less|css)\" --syntax less --fix",
    "copyFile": "node ./webpack/utils/copyFile.js",
