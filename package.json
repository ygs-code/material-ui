    "watchFile": "node  ./webpack/utils/watchFile.js",
    "test": "node  -r @babel/register    ./client/assets/js/request/test.js",
    "bin": "cross-env NODE_ENV=development       RENDER=ssr   node   -r @babel/register    ./bin/index.js",
