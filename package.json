    "start:ssr:dev": "cross-env     RENDER='ssr'     npx babel-node    -r @babel/register    ./bin/index.js    -r  dotenv/config  dotenv_config_path=.env.development",
    "build:ssr:prod": "cross-env  RENDER='ssr'  npx babel-node    -r @babel/register    ./bin/index.js   -r  dotenv/config  dotenv_config_path=.env.production",
    "start:ssr:prod": "cross-env  RENDER='ssr'  npx babel-node  -r  @babel/register    ./dist/server/index.js   -r  dotenv/config  dotenv_config_path=.env.production",
    "web:dev:server": "cross-env    RENDER=csr  webpack-dev-server   -r @babel/register      --config  ./webpack/config/client/index.js  ",
    "start:client:dev": "cross-env  RENDER=csr  npx babel-node    -r @babel/register    ./bin/index.js   -r  dotenv/config  dotenv_config_path=.env.development",
    "build:client:prod": "cross-env  RENDER=csr  npx babel-node    -r @babel/register    ./bin/index.js   -r  dotenv/config  dotenv_config_path=.env.production"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint"
    ],
    "*.{htm,html,css,sss,less,scss,saas}": [
      "npm run lint-style"
